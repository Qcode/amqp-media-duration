{
	"name": "silverorange/amqp_media_duration",
	"description": "AMQP worker to calculate media duration using ffpmeg.",
	"type": "library",
	"keywords": [ "ffprobe", "mp3", "m4a" ],
	"homepage": "https://github.com/silverorange/amqp-media-duration",
	"license": "MIT",
	"authors": [
		{
			"name": "Michael Gauthier",
			"email": "mike@silverorange.com"
		}
	],
	"bin": [
		"bin/media-duration.php"
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer",
			"options": {
				"ssl": {
					"cafile": "/etc/nginx/tls.crt/composer.crt"
				}
			}
		}
	],
	"require": {
		"php": ">=5.3.0",
		"psr/log": "^1.0.0",
		"silverorange/site": "^3.0.0"
	},
	"autoload": {
		"psr-0": {
			"AMQP_MediaDuration": ""
		}
	},
	"include-path": [
		"./"
	]
}
